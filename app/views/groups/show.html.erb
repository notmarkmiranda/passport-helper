<div class="container">
	<div class="page-header">
		<i class="fa fa-tachometer fa-2x active" aria-hidden="true"></i>
		<p><%= @group.name %></p>
		<span><%= link_to @group.passport.name, passport_path(@group.passport) %></span>
	</div>

	<table class="table table-bordered table-striped table-sm table-group">
		<thead class="thead-inverse">
			<tr>
				<th class="group-show-col-1">Venue</th>
				<% @users.each do |user| %>
					<th><%= user.name %></th>
				<% end %>
			</tr>
		</thead>
		<tbody>
			<% @venues.each do |venue| %>
				<tr>
					<td><%= venue.name %></td>
					<% @users.each do |user| %>
						<td id="<%= "#{venue.yelp_venue.yelp_id}-#{user.id}" %>"></td>
					<% end %>
				</tr>
			<% end %>
		</tbody>
	</table>
</div>

<script>
$(document).ready(function() {
	highlightVisits();
})

function highlightVisits(){
	$("<%= @visits %>").addClass('visited-cell')
}
</script
