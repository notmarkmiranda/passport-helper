<!DOCTYPE html>
<html>
<head>
  <title>PassportHelper</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
<%= render partial: "shared/navbar" %>

	<div class="container">
		<% flash.each do |key, value| %>
			<div class="alert alert-<%= key %>">
				<%= value %>
			</div>
		<% end %>
    <%= yield %>
	</div>
</body>
</html>
